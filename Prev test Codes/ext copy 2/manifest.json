{
    "manifest_version": 2,
    "name": "MeetMan",
    "version": "1.0",
    "description": "Collects captions from Google Meet",
    "permissions": ["activeTab", "storage"],
    "content_security_policy": {
    "extension_pages": "script-src 'self' https://www.gstatic.com/firebasejs/10.12.5/; object-srcÂ 'self'" 
  },
    "content_scripts": [
      {
        "matches": ["https://meet.google.com/*"],
        "js": ["link_checker.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["content.js"],
        "matches": ["https://meet.google.com/*"]
      }
    ]
  }