{
  "manifest_version": 3,
  "name": "MeetMan",
  "version": "1.0",
  "description": "Meetman provides seamless real-time chat and collaboration during meetings.",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["link_checker.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["content.js"],
      "matches": ["https://meet.google.com/*"]
    }
  ]
}